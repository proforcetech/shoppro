export class CreateEstimateDto {
  @IsUUID()
  vehicleId: string;

  @IsArray()
  @ValidateNested({ each: true })
  @Type(() => JobDto)
  jobs: JobDto[];

  @IsOptional()
  @IsEnum(EstimateStatus)
  status?: EstimateStatus;
}




